\n        actions[action]().then((data) => {\n            props.getHosts()\n            props.history.push(\"/dashboard/\")\n        })\n    }\n\n    return (\n        <div>\n            <form onSubmit={handleSubmit}>\n                <input type=\"text\" name=\"name\" value={formData.name} onChange={handleChange} />\n                <input type=\"text\" name=\"zipcode\" value={formData.zipcode} onChange={handleChange} />\n                <input type=\"text\" name=\"rate\" value={formData.rate} onChange={handleChange} />\n                <input type=\"text\" name=\"animals\" value={formData.animals} onChange={handleChange} />\n                <input type=\"submit\" value={action} />\n            </form>\n        </div>\n\n    )\n}\n\nexport default Form",{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":1,"column":8,"nodeType":"76","messageId":"77","endLine":1,"endColumn":12},{"ruleId":"74","severity":1,"message":"78","line":4,"column":17,"nodeType":"76","messageId":"77","endLine":4,"endColumn":21},{"ruleId":"74","severity":1,"message":"79","line":7,"column":8,"nodeType":"76","messageId":"77","endLine":7,"endColumn":11},{"ruleId":"74","severity":1,"message":"80","line":14,"column":8,"nodeType":"76","messageId":"77","endLine":14,"endColumn":13},{"ruleId":"74","severity":1,"message":"78","line":2,"column":10,"nodeType":"76","messageId":"77","endLine":2,"endColumn":14},{"ruleId":"74","severity":1,"message":"81","line":2,"column":9,"nodeType":"76","messageId":"77","endLine":2,"endColumn":19},{"ruleId":"82","severity":1,"message":"83","line":37,"column":13,"nodeType":"84","messageId":"85","endLine":37,"endColumn":18},{"ruleId":"82","severity":1,"message":"83","line":42,"column":13,"nodeType":"84","messageId":"85","endLine":42,"endColumn":18},{"ruleId":"82","severity":1,"message":"83","line":46,"column":13,"nodeType":"84","messageId":"85","endLine":46,"endColumn":18},{"ruleId":"82","severity":1,"message":