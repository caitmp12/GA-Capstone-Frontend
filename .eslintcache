r } = userData\n            dispatch({ type: \"auth\", payload: { token, username: user.username } })\n            window.localStorage.setItem(\n                \"auth\",\n                JSON.stringify({ token, username: user.username }))\n            props.history.push(\"/dashboard\")\n        }\n    }, [userData])\n\n    const actions = {\n        signup: () => {\n            return fetch(state.url + \"/users\", {\n                method: \"post\",\n                headers: {\n                    \"Content-Type\": \"application/json\",\n                },\n                body: JSON.stringify(formData),\n            }).then((response) => response.json())\n        },\n        login: () => {\n            return fetch(state.url + \"/login\", {\n                method: \"post\",\n                headers: {\n                    \"Content-Type\": \"application/json\",\n                },\n                body: JSON.stringify(formData),\n            }).then((response) => response.json())\n        }    //comma\n    }\n\n    const handleChange = (event) => {\n        setFormData({ ...formData, [event.target.name] : event.target.value })\n    }\n\n    const handleSubmit = (event) => {\n        event.preventDefault()\n        actions[type]().then((data) => {\n            setUserData(data)\n        })\n    }\n\n    return (\n        <div>\n            <form onSubmit={handleSubmit}>\n                <input type=\"text\" name=\"username\" value={formData.username} onChange={handleChange} />\n                <input type=\"text\" name=\"password\" value={formData.password} onChange={handleChange} />\n                <input type=\"submit\" value={type} />\n            </form>\n        </div>\n    )\n}\n\nexport default Auth","/Users/caitlinmoore-penaskovic/seir-flex-zen-work/projects/GA-capstone/Frontend/say-hi-app/src/pages/Nav.jsx",["65","66"],"import React from \"react\"\nimport { Route, Link, Switch } from \"react-router-dom\"\nimport { useAppState } from \"../AppState\"\n\nc