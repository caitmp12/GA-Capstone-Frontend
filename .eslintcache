entDefault()\n        actions[type]().then((data) => {\n            setUserData(data)\n        })\n    }\n\n    return (\n        <div>\n            <form onSubmit={handleSubmit}>\n                <input type=\"text\" name=\"username\" value={formData.username} onChange={handleChange} />\n                <input type=\"text\" name=\"password\" value={formData.password} onChange={handleChange}/>\n                <input type=\"submit\" value={type} />\n            </form>\n        </div>\n    )\n}\n\nexport default Auth","/Users/caitlinmoore-penaskovic/seir-flex-zen-work/projects/GA-capstone/Frontend/say-hi-app/src/components/Form.js",["52"],"import React from \"react\"\nimport { Link } from \"react-router-dom\"\n\nconst Form = (props) => {\n\n    return (\n        <h1>Form</h1>\n    )\n}\n\nexport default Form;",["53","54"],"/Users/caitlinmoore-penaskovic/seir-flex-zen-work/projects/GA-capstone/Frontend/say-hi-app/src/AppState.jsx",["55","56"],"import React from \"react\"\nimport {useContext, useReducer} from \"react\"\n\n\n//Initial State\n\nconst initialState = {\n    url: \"http://localhost:3000\",\n    token: null,\n    username: null\n}\n\n//Reducer\n\n//action = {type: \"\", payload: ---}\n\nconst reducer = (state, action) => {\n\n    switch(action.type){\n        case \"signup\":\n            fetch(\"http://localhost:3000/users/\", {\n                method: \"post\",\n                headers: {\n                    \"Content-Type\": \"application/json\"\n                },\n                body: JSON.stringify(action.payload)\n            })\n            .then(response => response.json())\n            .then(user => {\n                return {\n                    ...state,\n                    token: user.token,\n                    username: user.username,\n                }\n            })\n        break\n        case \"login\":\n            fetch(\"http://localhost:3000/login/\", {\n                method: \"post\",\n                headers: {\n                    \"Content-Type\": \"application/json\"\n                },\n                body: JSON.stringify(action.payload),\n            })\n                .then(response => response.json())\n                .then(user => {\n                    return {\n                        ...state,\n                        token: user.token,\n                        username: user.username,\n                    }\n                })  \n            break      \n        default:\n            return state \n            break\n    }\n\n}\n\n//App Context\n\nconst AppContext = React.createContext(null)\n\n//AppState Component\n\nexport const AppState = (props) => {\n\n    const [state, dispatch] = useReducer(reducer, initialState)\n\n    return <AppContext.Provider value={{state, dispatch}}>\n        {props.children}\n    </AppContext.Provider>\n\n}\n\n//useAppState Hook\n\nexport const useAppState = () => {\n    return React.useContext(AppContext)\n}\n","/Users/caitlinmoore-penaskovic/seir-flex-zen-work/projects/GA-capstone/Frontend/say-hi-app/src/components/Dashboard.js",[],"/Users/caitlinmoore-penaskovic/seir-flex-zen-work/projects/GA-capstone/Frontend/say-hi-app/src/components/Nav.js",["57","58"],"import React from \"react\"\nimport { Route, Link, Switch } from \"react-router-dom\"\n\nconst Nav = (props) => {\n    return (\n        <header>\n            <h1>sayHi!</h1>\n            <nav>\n                <Link to=\"/\">Home</Link>\n                <Link to=\"/auth/signup\">Signup</Link>\n                <Link to=\"/auth/login\">Login</Link>\n            </nav>\n        </header>\n    )\n}\n\nexport default Nav",{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":1,"column":8,"nodeType":"65","messageId":"66","endLine":1,"endColumn":12},{"ruleId":"63","severity":1,"message":"67","line":4,"column":17,"nodeType":"65","messageId":"66","endLine":4,"endColumn":21},{"ruleId":"63","severity":1,"message":"67","line":2,"colu